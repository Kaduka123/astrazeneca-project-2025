# .env.example
# CHO Analysis Environment Configuration (.env.example)
# Copy this file to .env and modify the values as needed.
# Lines starting with # are comments.

# --- Paths ---
# Defaults are usually fine unless your data/results are elsewhere relative to project root
# CHO_ANALYSIS_PATHS_DATA_DIR=data
# CHO_ANALYSIS_PATHS_RESULTS_DIR=results
# CHO_ANALYSIS_PATHS_RAW_DIR_NAME=raw
# CHO_ANALYSIS_PATHS_PREPROCESSED_DIR_NAME=preprocessed
# CHO_ANALYSIS_PATHS_FIGURES_DIR_NAME=figures
# CHO_ANALYSIS_PATHS_TABLES_DIR_NAME=tables
# CHO_ANALYSIS_PATHS_LOGS_DIR=logs

# --- Files ---
# Change if input filenames differ
# CHO_ANALYSIS_FILES_EXPRESSION_FILE=expression_counts.txt
# CHO_ANALYSIS_FILES_MANIFEST_FILE=MANIFEST.txt
# CHO_ANALYSIS_FILES_CDS_SEQUENCES_FILE=CDS_sequences.fasta
# CHO_ANALYSIS_FILES_UTR5_SEQUENCES_FILE=5UTR_sequences.fasta
# CHO_ANALYSIS_FILES_UTR3_SEQUENCES_FILE=3UTR_sequences.fasta

# --- Batch Correction ---
# CHO_ANALYSIS_BATCH_CORRECTION_METHODS=combat # Only combat is fully implemented
# CHO_ANALYSIS_BATCH_CORRECTION_DEFAULT_METHOD=combat
# CHO_ANALYSIS_BATCH_CORRECTION_BATCH_KEY=platform # Use 'platform' derived from manifest description
# CHO_ANALYSIS_BATCH_CORRECTION_PRESERVE_TARGET_GENE=true
# CHO_ANALYSIS_BATCH_CORRECTION_TARGET_COL=PRODUCT-TG
# CHO_ANALYSIS_BATCH_CORRECTION_RUN_ADVANCED=false # Set to true to run residual checks
# CHO_ANALYSIS_BATCH_CORRECTION_ADVANCED_ALPHA=0.01
# CHO_ANALYSIS_BATCH_CORRECTION_ADVANCED_METHOD=location_scale # Placeholder

# --- Correlation ---
# CHO_ANALYSIS_CORRELATION_TARGET_GENE=PRODUCT-TG
# CHO_ANALYSIS_CORRELATION_METHODS=pearson,spearman # Example: Use only these two
# CHO_ANALYSIS_CORRELATION_DEFAULT_METHOD=spearman
# CHO_ANALYSIS_CORRELATION_MIN_CORRELATION=0.5 # Filter for ranking/consensus
# CHO_ANALYSIS_CORRELATION_MAX_P_VALUE=0.05   # Filter for ranking/consensus
# CHO_ANALYSIS_CORRELATION_TOP_N=50 # Number of top genes for ranking output list
# CHO_ANALYSIS_CORRELATION_RUN_BOOTSTRAP=true # true/false
# CHO_ANALYSIS_CORRELATION_BOOTSTRAP_ITERATIONS=100 # Reduce for faster testing, e.g., 100
# CHO_ANALYSIS_CORRELATION_CONFIDENCE_LEVEL=0.95
# CHO_ANALYSIS_CORRELATION_RUN_PANEL_OPTIMIZATION=true # true/false
# CHO_ANALYSIS_CORRELATION_PANEL_SIZES=3,5,10
# CHO_ANALYSIS_CORRELATION_PANEL_TYPES=minimal_redundancy,max_score
# CHO_ANALYSIS_CORRELATION_PANEL_CANDIDATES=100

# --- Sequence Analysis ---
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_MIN_EXPRESSION_QUANTILE=0.9 # Currently used for consistency check
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_CONSTANT_EXPRESSION_CV_THRESHOLD=0.3 # Currently used for consistency check
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_CODON_USAGE_METHODS=relative_adaptiveness,cai,rscu
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_ANALYZE_5UTR=true
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_ANALYZE_3UTR=true
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_ANALYZE_CDS=true
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_REGEX_KOZAK=GCCACC|CCACC
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_REGEX_TOP=^C[CT]{4,15}
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_REGEX_G_QUAD=G{3,}.{1,7}G{3,}.{1,7}G{3,}.{1,7}G{3,}
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_REGEX_POLYA=AATAAA
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_REGEX_ARE=ATTTA
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_REGEX_MIRNA_SEED=A[ACGT]{6}A

# --- Feature Significance Analysis ---
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_RUN_FEATURE_SIGNIFICANCE=true # Enable/disable statistical significance analysis
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_SIGNIFICANCE_ALPHA=0.05 # Significance threshold for statistical tests
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_MULTIPLE_TESTING_CORRECTION=fdr_bh # Multiple testing correction method (fdr_bh, bonferroni)

# --- Comparative Sequence Analysis ---
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_RUN_COMPARATIVE_ANALYSIS=true # Enable/disable comparative analysis
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_COMPARATIVE_REFERENCE_TYPE=high_expression # Reference profile type for comparison

# --- Sequence-Based Expression Prediction ---
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_RUN_EXPRESSION_PREDICTION=true # Enable/disable sequence-based prediction
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_PREDICTION_TARGET=cv # Target metric to predict: 'cv' (stability) or 'mean' (expression level)
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_PREDICTION_MODEL_TYPE=ensemble # Model type: 'linear', 'tree', or 'ensemble'
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_FEATURE_SELECTION_METHOD=statistical # Feature selection: 'statistical', 'rfe', or 'lasso'
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_MAX_FEATURES=15 # Maximum number of features to use for prediction

# --- Transgene Design ---
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_RUN_TRANSGENE_DESIGN=true # Enable/disable transgene design recommendations
# CHO_ANALYSIS_SEQUENCE_ANALYSIS_OPTIMIZATION_TARGET=cv_minimization # 'cv_minimization' (stability) or 'mean_maximization' (expression)

# --- Visualization ---
# CHO_ANALYSIS_VISUALIZATION_STYLE=seaborn-v0_8-whitegrid
# CHO_ANALYSIS_VISUALIZATION_DEFAULT_FIGSIZE=10,6
# CHO_ANALYSIS_VISUALIZATION_DEFAULT_DPI=300
# CHO_ANALYSIS_VISUALIZATION_SAVE_FIGURES=true
# CHO_ANALYSIS_VISUALIZATION_FIGURE_FORMAT=png
# CHO_ANALYSIS_VISUALIZATION_FONT_FAMILY=DejaVu Sans

# --- Logging ---
# Log levels: DEBUG, INFO, WARNING, ERROR, CRITICAL
# CHO_ANALYSIS_LOGGING_LEVEL=INFO
# CHO_ANALYSIS_LOGGING_FILE_LOGGING=true
# CHO_ANALYSIS_LOGGING_CONSOLE_LOGGING=true
# CHO_ANALYSIS_LOGGING_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
# CHO_ANALYSIS_LOGGING_ROOT_LOGGER_NAME=cho_analysis

# --- Performance ---
# CHO_ANALYSIS_PERFORMANCE_MAX_CPU_CORES=0 # 0 means use all available
# CHO_ANALYSIS_PERFORMANCE_LOW_MEMORY_MODE=false
# CHO_ANALYSIS_PERFORMANCE_CACHE_RESULTS=true
# CHO_ANALYSIS_PERFORMANCE_CACHE_DIR=.cache
